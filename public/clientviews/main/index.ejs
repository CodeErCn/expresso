<html>
	<head>
		<title><%= title %></title>
	 	<!-- load external script -->			
		<script src="/socket.io/socket.io.js"></script>
		<script type="text/javascript" src="./js/external/angular.min.js">
		</script>
		<script type="text/javascript" src="./js/external/jquery.min.js">
		</script>
		<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.16/angular-route.min.js">
		</script>		
	 	<!-- stylesheets -->		
		<link rel="stylesheet" type="text/css" href="./css/bootstrap.min.css">
		<link rel="stylesheet" type="text/css" href="./css/style.css">
		<!-- angular app, model, and controllers -->
		<script type="text/javascript" src="./js/app.js">
		</script>
		<script type="text/javascript" src="./js/factories/model.js"></script>
		<script type="text/javascript" src="./js/controllers/controller.js"></script>
	 <!-- io script -->
    <script type="text/javascript">
		io = io.connect();
		io.emit('client_sent', {msg: 'Client has emitted.'});
		io.on('server_sent', function(received){
			console.log(received);
		});
    </script>
	 <!-- jquery script -->
	<script type="text/javascript">
		$( document ).ready(function(){
	 		console.log('Jquery is ready.');
	    });
	</script>
	</head>
	<body>
		<h1><%= title %></h1>
		<div class='container' id='app' ng-app='App'>
			<div class='row'>
				<div class='col-md-offset-2 col-md-8'>
					<div class='links'>
						<a href="#/parta">Initial Partial</a> | <a href="#/partb">Another Partial</a>
					</div>
					<div ng-view=""></div>
				</div>
			</div>
		</div>
	</body>
</html>